{
  "name": "vitest-types",
  "version": "0.0.0-PLACEHOLDER",
  "types": "./dist/2.d.ts",
  "exports": {
    ".": {
      "types": "./dist/2.d.ts"
    },
    "./globals": {
      "types": "./dist/2/globals.d.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts"
    },
