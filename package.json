  },
  "files": ["dist"],
  "scripts": {
    "build": "pnpm --package tsx@4.19.2 dlx tsx build.ts",
    "prepare": "pnpm --package husky@9.1.1 dlx husky",
    "prepack": "pnpm build",
    "deploy": "npm pack"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@microsoft/api-extractor": "^7.48.1",
